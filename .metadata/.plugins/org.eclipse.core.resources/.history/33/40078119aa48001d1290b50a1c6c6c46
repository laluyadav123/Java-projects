package com.oppsdemo;

public class ClassA {
	
	public void method1() {
		
		System.out.println("inside parent class");
	}

public class ClassB extends ClassA {
	
	 public void method2() {
		 System.out.println("inside child class");
		 
	 }

}



	public static void main(String[] args) {
		
		ClassB b = new ClassB();
		b.method1();
		b.method2();
		
		
		

	

}
