package com.oppsdemo;

public class InheritanceDemo {

	public static void main(String[] args) {
		
		ClassC c = new ClassC();
		c.method3();
		
		
		
		ClassB b = new ClassB();
		b.method2();
	
	}

}
/*
   base class /parent class/super class 
   
   sub class/child class/ extended class 
   
   single inheritance
   
       classA
       		 extends	
       class B
       
   hirerical inheritance
   
       			ClassA
         extends			extends
       ClassB            ClassC			
       
       
       
       
       
       
       
       
       
       
 */