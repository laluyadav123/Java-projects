package com.oppsdemo;

public class InheritanceDemo {

	public static void main(String[] args) {
		
		ClassA a = new ClassA(2000);
		a.getSalary();
		System.out.println("ClassA salary :"+a);
		
		ClassC c = new ClassC(10000);
		c.getSalary();
		System.out.println("ClassC salary :"+c);
		
		
	}

}
/*
   base class /parent class/super class 
   
   sub class/child class/ extended class 
   
   single inheritance
   
       classA
       		 extends	
       class B
       
   hirerical inheritance
   
       			ClassA
         extends			extends
       ClassB            ClassC			
       
       
       
       
       
       
       
       
       
       
 */